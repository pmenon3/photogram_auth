<h3>
  <%= current_user.username %>
</h3>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Photos</th>
        <th>Comments</th>

      </tr>

      <% @my_likes.each do |my_like| %>
      <tr>
        <td><img src="<%= my_like.photo.image %>"></td>
         <td><%= my_like.photo.caption %></td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
